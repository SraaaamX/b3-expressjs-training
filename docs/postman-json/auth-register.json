{
  "url": "http://localhost:3000/api/auth/register",
  "method": "POST",
  "header": {
    "Content-Type": "multipart/form-data"
  },
  "body": {
    "mode": "formdata",
    "formdata": [
      {
        "key": "name",
        "value": "John Doe",
        "type": "text"
      },
      {
        "key": "nickname",
        "value": "johndoe",
        "type": "text"
      },
      {
        "key": "email",
        "value": "john@example.com",
        "type": "text"
      },
      {
        "key": "password",
        "value": "password123",
        "type": "text"
      },
      {
        "key": "role",
        "value": "user",
        "type": "text"
      },
      {
        "key": "profilepic",
        "type": "file",
        "src": "/path/to/your/image.jpg"
      }
    ]
  }
}