{
  "url": "http://localhost:3000/api/properties/{{propertyId}}",
  "method": "PUT",
  "header": {
    "Content-Type": "multipart/form-data",
    "Authorization": "Bearer {{token}}"
  },
  "body": {
    "mode": "formdata",
    "formdata": [
      {
        "key": "price",
        "value": "260000",
        "type": "text"
      },
      {
        "key": "description",
        "value": "Bright apartment with city view and close to shops",
        "type": "text"
      },
      {
        "key": "image",
        "type": "file",
        "src": "/path/to/your/image.jpg"
      }
    ]
  }
}