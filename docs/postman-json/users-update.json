{
  "url": "http://localhost:3000/api/users/{{userId}}",
  "method": "PATCH",
  "header": {
    "Content-Type": "multipart/form-data",
    "Authorization": "Bearer {{token}}"
  },
  "body": {
    "mode": "formdata",
    "formdata": [
      {
        "key": "name",
        "value": "John Updated",
        "type": "text"
      },
      {
        "key": "phone",
        "value": "0123456789",
        "type": "text"
      },
      {
        "key": "profilepic",
        "type": "file",
        "src": "/path/to/your/image.jpg"
      }
    ]
  }
}